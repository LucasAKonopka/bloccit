<h1>All Posts</h1>
<% @posts.each_with_index do |post , i| %>
  <div class = "media">
    <div class="media-body">
      <h4 class="media-heading">
        <% if (i + 1) % 5 != 0 %>
          <%= link_to post.title, post %>
        <% else %>
          <%= link_to "SPAM", post %>
        <% end %>
      </h4>
    </div>
  </div>
<% end %>
  